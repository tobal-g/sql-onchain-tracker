# Zapper API (required for portfolio queries)
ZAPPER_API_KEY=xxxx

# Database (Neon.tech PostgreSQL) - required for sync and manual entry features
DATABASE_URL=postgresql://user:pass@ep-xxx.us-east-2.aws.neon.tech/dbname?sslmode=require

# Database schema (optional, default 'public')
DATABASE_SCHEMA=public

# Sync endpoint authentication (optional, if set endpoint requires x-api-key header)
SYNC_API_KEY=your-secret-key-here

# Rate limiting between wallet syncs in milliseconds (optional, default 1000)
SYNC_RATE_LIMIT_MS=1000

# CORS origin for frontend (optional, default 'http://localhost:5173')
CORS_ORIGIN=http://localhost:5173

# Server port (optional, default 3000)
# PORT=3000

# Cache TTL in seconds (optional, default 90)
# CACHE_TTL=90